generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./../../../example.db"
}

model alembic_version {
  version_num String @id
}

model notes {
  id          Int     @default(autoincrement()) @id
  title       String?
  description String?
  author      String?
}

model users {
  id       Int     @default(autoincrement()) @id
  username String?
  password String?
  email    String?
  about    String?
}
